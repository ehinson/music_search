    <div class="site-wrapper">

      <div class="site-wrapper-inner">

        <div class="cover-container">

          <div class="inner cover">

            Listing: 
              <%= params['search'] %> <br>
            Listing: 
              <%=  params['artist']%><br>
            Listing:
              <% @artists.map(&:name).each do |artist|  %>
                <%= artist  %><br>
              <%  end  %>
            
            Listing: 

              <% @similar_tracks.map(&:name).each do |similar|  %>
                <%= similar  %> Similar!
              <%  end  %>

              <%= @tracks.count %> tracks <br>
              <% @tracks.each do |track| %>
                <%= track[:name] %>
                <strong><%= track[:artist] %></strong><br>
              <%  end  %>              
          </div>
        </div>
      </div>
    </div>
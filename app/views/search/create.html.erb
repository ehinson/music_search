    <div class="site-wrapper">

      <div class="site-wrapper-inner skin-1">

        <div class="cover-container container">

          <div class="content">
            <div class="inner cover grid cloud">
              <h3 class= "lead"><span class="pull-left">Listing: <%= params['search']['artist'] %></span><span class="pull-right"><script type="text/javascript"> document.write(Math.floor(Math.random()* 10000)+ 1)</script> results<span> 
              </h3>
            </div><!--inner-->
            <div id="tabs" class="tabs">
              <nav>
                <ul class="nav nav-tabs">
                  <li class='<% 'active' if params[:tab].nil?  %>'><a href="create.html" class="icon-1 text-uppercase breathe"><span>Artists</span></a></li>
                  <li><a href="#section-2" class="icon-2 text-uppercase breathe"><span>Genres</span></a></li>
                  <li><a href="#section-3" class="icon-3 text-uppercase breathe"><span>Albums</span></a></li>
                  <li><a href="songs.html" class="icon-4 text-uppercase breathe"><span>Songs</span></a></li>
                  <li><a href="#section-5" class="icon-5 text-uppercase breathe"><span>Playlists</span></a></li>
                </ul>
              </nav>

            </div><!--tabs-->
            <section id= "section-2">
              <div>
                <% @artists.each do |artist| %>
                <div class="result one-fifth ">
                  <div class="feature-image"><img src="http://placehold.it/150x150"></div>
                  <div class="info"><%= params['search']['artist'] %>
                    <%= artist.name %>
                    <%= params['search']['song'] %>
                    <%= params['search']['hotttness'] %></div>
                    <div class="info2">
                      <div class="one-third left"> <strong>Text</strong><br>Text</div>
                      <div class="one-third middle">  <strong>Text</strong><br>Text</div>
                      <div class="one-third right"> <strong>Text</strong><br>Text</div>
                    </div>
                  </div>
                  <%  end  %>

                </div>
              </div>
            </div><!--mediabox-->
          </section>
        </div>


      </div><!--cover-container-->

    </div><!--site-wrapper-inner-->
  </div><!--site-wrapper-->
